{
  "name": "strider-github",
  "version": "2.4.0",
  "description": "A GibHub & GitHub Enterprise provider for Strider",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run lint && npm run tests",
    "lint": "eslint lib",
    "tests": "mocha --timeout 5000 -R spec test/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Strider-CD/strider-github.git"
  },
  "keywords": [
    "git",
    "strider"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "author": "Jared Forsyth <jared@jaredforsyth.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "strider": {
    "id": "github",
    "title": "Github",
    "type": "provider",
    "hosted": true,
    "config": {
      "controller": "GithubCtrl"
    },
    "accountConfig": {
      "setupLink": "/auth/github"
    },
    "webapp": "lib/webapp.js",
    "worker": "lib/worker.js",
    "inline_icon": "github"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "expect.js": "~0.2.0",
    "mocha": "^5.2.0",
    "nock": "^9.0.22"
  },
  "dependencies": {
    "async": "^1.5.2",
    "debug": "^2.6.9",
    "gravatar": "^1.0.6",
    "lodash": "^4.17.15",
    "passport-github": "^1.1.0",
    "scmp": "0.0.2",
    "ssh-keypair": "^1.0.0",
    "step": "^1.0.0",
    "strider-git": "*",
    "superagent": "^5.1.0"
  }
}
